        "type": "{\n  \"field\": (\n    \"transaction hash\"\n    or \"receipt\"\n    or \"confirmation\"\n    or \"contract\"\n    or \"error\"\n  ),\n  \"data\": any\n}",
        "name": "events"
      },
      "026c4fcf-30bb-480f-9488-5da56ebea5be": {
        "type": "{\"error\": string}",
        "name": "error"
      }
    }
  },
