        "name": "params",
        "description": "https://docs.nftport.xyz/docs/nftport/b3A6MjA0MjY5MjE-multi-chain-nft-search#Request\n\nDefault chain value: \"all\""
      }
    },
    "outputs": {
      "c2521e42-e908-4135-bac8-a6ea10fe79cf": {
        "type": "{\n\n  \"response\":\"OK\",\n  \"search_results\": [\n    {\n      \"chain\": (\"polygon\" or \"ethereum\"),\n      \"contract_address\": \"string\",\n      \"token_id\": string,\n      \"name\": string,\n      \"description\": string,\n       optional \"mint_date\": string\n    }\n  ]\n}",
        "name": "data",
        "description": "https://docs.nftport.xyz/docs/nftport/b3A6MjE0MDYzODI-list-all-your-minted-nf-ts#Responses"
      },
      "384bbd6b-c13e-4fed-9724-336637c51b87": {
        "type": "{\n  \"status\": number,\n  \"headers\": {string: string},\n  \"body\": string\n}",
        "name": "response",
        "description": "Sends the original response."
      },
      "e14e707e-19d7-4c5a-8408-dca6b0227725": {
        "type": "{\n  \"error\": string,\n  \"details\": any\n}",
        "name": "error",
        "description": ""
      }
    }
