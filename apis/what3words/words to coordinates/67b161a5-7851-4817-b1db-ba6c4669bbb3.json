{
  "name": "apis/what3words/Words to coordinates",
  "description": "Converts what3words word triple to geo coordinates.\n\nMore:\nhttps://what3words.com",
  "interface": {
    "inputs": {
      "60d3a7aa-566c-4f9d-8b42-d7929acd5d5a": {
        "type": "[string, string, string]",
        "name": "words",
        "description": "Receives a word triple that identifies any 3m x 3m area on the surface of the Earth.\n\nExample:\n[\"pretty\", \"needed\", \"chill\"]"
      },
      "222ace49-5d75-4ec8-b2ae-4cc4c7299a5f": {
        "type": "{\n  \"apiKey\": string,\n  optional \"format\": (\"json\" or \"geojson\")\n}",
        "name": "params",
        "description": "Receives what3words API parameters."
      }
    },
    "outputs": {
      "c845be73-ee70-4429-ae6c-403d8a221c76": {
        "type": "({\n  \"country\": string,\n  \"square\": {\n      \"southwest\": {\n          \"lng\": number,\n          \"lat\": number\n      },\n      \"northeast\": {\n          \"lng\": number,\n          \"lat\": number\n      }\n  },\n  \"nearestPlace\": string,\n  \"coordinates\": {\n      \"lng\": string,\n      \"lat\": string\n  },\n  \"words\": string,\n  \"language\": string,\n  \"map\": string\n}\nor\n{\n  \"features\": [\n      {\n          \"bbox\": [number, number, number, number],\n          \"geometry\": {\n              \"coordinates\": [number, number],\n              \"type\": \"Point\"\n          },\n          \"type\": \"Feature\",\n          \"properties\": {\n              \"country\": string,\n              \"nearestPlace\": string,\n              \"words\": string,\n              \"language\": string,\n              \"map\": string\n          }\n      }\n  ],\n  \"type\": \"FeatureCollection\"\n})",
        "name": "data",
        "description": "Sends data structure describing the geographical area identified by the 3-word address."
      },
      "a0f3f6d6-1798-48e9-b2a0-1776a2b92760": {
        "type": "{\n  \"status\": number,\n  \"headers\": {string: any},\n  \"body\": string\n}",
        "name": "response",
        "description": ""
      },
      "a722f3ba-ea17-4e5c-8dd1-537a094a8b9d": {
        "type": "{\n  \"error\": string,\n  optional \"details\": any\n}",
        "name": "error"
      }
    }
  },
