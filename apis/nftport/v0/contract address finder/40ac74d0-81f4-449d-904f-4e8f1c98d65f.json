{
  "name": "apis/nftport/v0/Contract address finder",
  "description": "Finds a contract address based on the hash of the transaction that deployed the contract.\n\nRetrieve a deployed contract:\nhttps://docs.nftport.xyz/docs/nftport/b3A6MjE2NjM4OTk-retrieve-a-deployed-contract",
  "interface": {
    "inputs": {
      "0ca6cf73-3922-45b4-bc45-e257cf6d5047": {
        "type": "string",
        "name": "transaction_hash",
        "description": "Receives the hash for the transaction that deployed the smart contract.\n\nhttps://docs.nftport.xyz/docs/nftport/b3A6MjE2NjM4OTk-retrieve-a-deployed-contract#Path-Parameters"
      },
      "af69ae31-f9c6-43a1-b184-41de1f19d0b2": {
        "type": "{\n  \"api_key\": string,\n  optional \"chain\": (\"rinkeby\" or \"polygon\" or \"ethereum\") \n}",
        "name": "params",
        "description": "https://docs.nftport.xyz/docs/nftport/b3A6MjE2NjM4OTk-retrieve-a-deployed-contract#Query-Parameters\n\nDefault chain value: \"polygon\""
      }
    },
    "outputs": {
      "6a557de8-cac3-4a74-8a33-25c2fca5f4f5": {
        "type": "{\n  \"response\": \"OK\",\n  \"chain\": (\"polygon\" or \"rinkeby\"  or \"ethereum\"),\n  \"contract_address\": string,\n  \"transaction_hash\": string\n}",
        "name": "data",
        "description": "Sends the details of the contract that was deployed in the specified transaction."
      },
      "5445f777-9824-43a6-8306-dcb941471807": {
        "type": "{\n  \"status\": number,\n  \"headers\": {string: any},\n  \"body\": string\n}",
        "name": "response",
        "description": "Sends the original response."
      },
      "b3f88e13-ec3c-4d84-b6ee-c55d15bb19d5": {
        "type": "{\n  \"error\": string,\n  \"details\": any\n}",
        "name": "error"
      }
    }
  },
