      },
      "55e933ac-c051-439a-9695-c6a5d0c0940e": {
        "type": "{\n  \"api_key\": string,\n  optional \"chain\": (\"polygon\" or \"rinkeby\" or \"ethereum\")\n}",
        "name": "params",
        "description": "Reference:\nhttps://docs.nftport.xyz/docs/nftport/b3A6Mzk2MjA1MzQ-deploy-an-nft-collection-contract#request-body\n\nDefault chain value: \"polygon\""
      }
    },
    "outputs": {
      "284d7025-55ce-4d59-ae7b-9e14b37904d5": {
        "type": "{\n  \"response\": \"OK\",\n  \"chain\": (\"polygon\" or \"rinkeby\" or \"ethereum\"),\n  \"transaction_hash\": string,\n  \"transaction_external_url\": string,\n  \"owner_address\": string,\n  \"type\": (\"erc721\" or \"erc1155\"),\n  \"name\": string,\n  \"symbol\": string\n}",
        "name": "data",
        "description": "Sends details of the deployed contract.\n\nReference:\nhttps://docs.nftport.xyz/docs/nftport/b3A6Mzk2MjA1MzQ-deploy-an-nft-collection-contract#response-body"
