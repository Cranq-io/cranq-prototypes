{
  "name": "apis/airtable/bulk records updater",
  "description": "Updates multiple specific records in the specified AirTable table. The input received on parameter records must match the target table schema.\r\n\r\nSends records to the AirTable API in batches of 10.\r\n\r\nFor detailed parameter information, see the AirTable API documentation: \r\nhttps://airtable.com/api/meta",
  "interface": {
    "inputs": {
      "0b5512e3-e4e1-418d-97fb-82aa6fff413c": {
        "type": "[\n  {\n    \"id\" : \"\",\n    \"fields\" : {\n      \"field\" : \"\"\n    }\n  }\n]",
        "name": "records",
        "description": "Receives arbitrary number of records to be inserted into AirTable. Number of records inserted must be greater than 1."
      },
      "ae700e3b-8a74-4562-ab82-64d9c6e1cdea": {
        "type": "{\n  \"apiKey\": string,\n  \"baseId\": string,\n  \"tableName\": string\n}",
        "name": "params",
        "description": "API Key, Base ID and Table Name all extracted from AirTable\r\n\r\nAPI Key: Go into AirTable account and scroll to '<>API'to find the\r\n\r\nBase ID: In AirTable, go to 'Help?' and click on 'API Documentation'. Find the ID of the base in the introduction.\r\n\r\nTable Name: Use the table name located on the tab of the spreadsheet"
      }
    },
    "outputs": {
      "217fdce8-4a09-4f9f-b429-5096aec6140c": {
        "type": "[\n  {\n    \"field\": \"\",\n    \"Calculation\": \"record id\"\n  }\n]",
        "name": "records",
        "description": "The original changes to be updated in the AirTable.\r\n\r\nIncludes updated record values and record IDs."
      },
      "c2dbd9ed-e12e-454a-a6a7-3258856e92a8": {
        "type": "[\n  {\n    \"id\": \"\",\n    \"createdTime\": \"\",\n    \"fields\": {\n      \"\": \"\",\n      \"Calculation\": \"\"\n    }\n  }\n]",
        "name": "AT records",
        "description": "Updated records as is sent to the AirTable API.\r\n\r\nIncludes record IDs, created times of records and updated record values"
      },
      "13f44ddb-a5bb-4047-924c-98273b574724": {
        "type": "{\n  \"status\": number,\n  \"headers\": {string: any},\n  \"body\": string\n}",
        "name": "responses",
        "description": "Sends the entire response from the AirTable API without modification.\r\n\r\nSee link for correct formats of request and response 'update record' messages:\r\nhttps://airtable.com/appJrRnzGR3Yp5M7b/api/docs#curl/table:projects:update"
      },
      "2deb0dba-02fc-4fb2-87c2-0889ae38140a": {
        "type": "{\n  \"error\": string,\n  optional \"details\": any\n}",
        "name": "error"
      }
    }
  },
